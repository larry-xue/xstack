{
  "name": "@repo/api-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --outDir dist --clean",
    "generate": "openapi-typescript ./openapi/openapi.json -o ./src/generated/schema.ts",
    "typecheck": "node ../../node_modules/typescript/bin/tsc -p tsconfig.json --noEmit",
    "lint": "pnpm -w lint",
    "test": "pnpm run typecheck"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3"
  }
}
